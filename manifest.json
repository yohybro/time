{
    "manifest_version":2,
    "name":"Timezone",
    "version":"2.0.1",
    "description":"This is a simple extension that displays your Timezone",
    "icons":{
        "128":"clock.png"
    },
    "browser_action":{
    "default_popup":"popup.html",
    "default_icon":"clock.png",
    "default_title":"Timezone"
    },
    "permissions":["activeTab"],
    "background":{
        "scripts":["main.js"]
    }
}
